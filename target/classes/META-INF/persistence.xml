<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="bazaDanychSeriwsu" transaction-type="RESOURCE_LOCAL">
        <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
        <class>Model.TAdresEntity</class>
        <class>Model.TKlientEntity</class>
        <class>Model.TKontaktEntity</class>
        <class>Model.TSprzetEntity</class>
        <class>Model.TZlecenieEntity</class>
        <properties>
            <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="javax.persistence.jdbc.user" value="postgres"/>
            <property name="javax.persistence.jdbc.password" value="password"/>
            <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/ManagerSerwisu2"/>
            <property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQL95Dialect"/>
            <property name="hibernate.hbm2ddl.auto" value="create"/>
            <property name="toplink.jdbc.url" value="jdbc:postgresql://localhost:5432/ManagerSerwisu2"/>
            <property name="toplink.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="hibernate.connection.url" value="jdbc:postgresql://localhost:5432/ManagerSerwisu2"/>
            <property name="hibernate.connection.driver_class" value="org.postgresql.Driver"/>
            <property name="openjpa.ConnectionURL" value="jdbc:postgresql://localhost:5432/ManagerSerwisu2"/>
            <property name="openjpa.ConnectionDriverName" value="org.postgresql.Driver"/>
            <property name="eclipselink.jdbc.url" value="jdbc:postgresql://localhost:5432/ManagerSerwisu2"/>
            <property name="eclipselink.jdbc.driver" value="org.postgresql.Driver"/>
        </properties>
    </persistence-unit>
</persistence>
